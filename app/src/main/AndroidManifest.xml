<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.yzs.yzslibrarydemo">

    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:name=".app.YzsApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".activity.DemoActivity"
            android:launchMode="singleTask"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".activity.FirstViewActivity"
            android:screenOrientation="portrait" />

        <activity
            android:name=".activity.AdaptiveWebViewActivity"
            android:screenOrientation="portrait" />

        <activity
            android:name=".activity.StateButtonActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activity.LikeIosDialogActivity"
            android:screenOrientation="portrait" />


    </application>

</manifest>